{
	"name": "@cloudflare/containers-shared",
	"version": "0.1.0",
	"description": "Package that contains shared container functionality for Cloudflare Workers SDK.",
	"keywords": [
		"cloudflare",
		"containers",
		"cloudchamber",
		"docker"
	],
	"homepage": "https://github.com/cloudflare/workers-sdk/tree/main/packages/containers-shared#readme",
	"bugs": {
		"url": "https://github.com/cloudflare/workers-sdk/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cloudflare/workers-sdk.git",
		"directory": "packages/containers-shared"
	},
	"license": "MIT OR Apache-2.0",
	"author": "wrangler@cloudflare.com",
	"types": "./dist",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pnpm run clean && pnpm run types:emit",
		"check:lint": "eslint . --max-warnings=0",
		"check:type": "tsc",
		"clean": "rimraf dist",
		"dev": "pnpm run clean && pnpm run types:emit --watch",
		"test": "vitest",
		"test:ci": "vitest --run",
		"types:emit": "tsc index.ts --declaration --emitDeclarationOnly --declarationDir ./dist --skipLibCheck --target ES2022 --moduleResolution bundler"
	},
	"dependencies": {
		"undici": "^7.10.0"
	},
	"devDependencies": {
		"@cloudflare/eslint-config-worker": "workspace:*",
		"@cloudflare/workers-tsconfig": "workspace:*",
		"@types/node": "catalog:node18",
		"rimraf": "catalog:default",
		"typescript": "catalog:default",
		"undici-types": "^6.20.0",
		"vitest": "~2.1.0"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"volta": {
		"extends": "../../package.json"
	}
}
